package com.example.domain;

public class Employee {

	//	ID
	private Integer id;
	//	名前
	private String name;
	//	年齢
	private Integer age;
	//	性別
	private String gender;
	//	部署ID
	private Integer departmentId;
	
	@Override
	public String toString() {
		return "Employee [id=" + id + ", name=" + name + ", age=" + age + ", gender=" + gender + ", departmentId="
				+ departmentId + "]";
	}
	
	
}
